@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix osc: <http://lv2plug.in/ns/ext/osc#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://lv2plug.in/ns/ext/osc>
	a owl:Ontology ;
	rdfs:seeAlso <osc.h> ,
		<lv2-osc.doap.ttl> ;
	lv2:documentation """
<p>TODO.</p>
""" .

osc:Event
	a rdfs:Class ,
		rdfs:Datatype ;
	rdfs:subClassOf atom:Atom ;
	rdfs:label "OSC Event" ;
	lv2:documentation """
<p>TODO.</p>
""" .

osc:Timestamp
	a rdfs:Class ,
		rdfs:Datatype ;
	rdfs:subClassOf atom:Atom ;
	rdfs:label "OSC Timestamp" ;
	atom:cType "LV2_OSC_Timestamp" ;
	rdfs:comment "Two unsigned Int representing a 32.32 fixed point OSC timestamp." .

osc:Bundle
	a rdfs:Class ,
		rdfs:Datatype ;
	rdfs:subClassOf atom:Object ;
	rdfs:label "Bundle" ;
	rdfs:comment "An atomized OSC bundle object." .

osc:Message
	a rdfs:Class ,
		rdfs:Datatype ;
	rdfs:subClassOf atom:Object ;
	rdfs:label "Message" ;
	rdfs:comment "An atomized OSC message object." .

osc:Packet
	a rdfs:Class ,
		rdfs:Datatype ;
	rdfs:subClassOf atom:Atom ;
	owl:onDatatype xsd:base64Binary ;
	rdfs:label "Packet" ;
	rdfs:comment "A raw OSC packet." .

osc:bundleTimestamp
	a rdfs:Class ;
	rdfs:subClassOf atom:Property ,
		osc:Timestamp ;
	rdfs:label "Bundle timestamp" ;
	rdfs:comment "Timestamp of OSC bundle." .

osc:bundleItems
	a rdfs:Class ;
	rdfs:subClassOf atom:Property ,
		atom:Tuple ;
	rdfs:label "Bundle items" ;
	rdfs:comment "Items of OSC bundle." .

osc:messagePath
	a rdfs:Class ;
	rdfs:subClassOf atom:Property ,
		atom:String ;
	rdfs:label "Message path" ;
	rdfs:comment "Path of OSC message." .

osc:messageArguments
	a rdfs:Class ;
	rdfs:subClassOf atom:Property ,
		atom:Tuple ;
	rdfs:label "Message arguments" ;
	rdfs:comment "Arguments of OSC message." .
